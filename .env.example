# Copy to .env and fill real values before running docker-compose

# Postgres settings
POSTGRES_DB=lmsdb
POSTGRES_USER=lmsuser
POSTGRES_PASSWORD=change_me_to_strong_password

# Domains (used for reference in nginx configs)
ADMIN_HOST=admin.example.com
STUDENT_HOST=student.example.com

# SSL filenames (place certificates in ./nginx/certs)
ADMIN_CRT=admin.crt
ADMIN_KEY=admin.key
STUDENT_CRT=student.crt
STUDENT_KEY=student.key
